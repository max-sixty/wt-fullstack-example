# Example environment file for full-stack development
# This file is checked into git. Actual .env files are generated per-worktree.

# Backend server configuration
BACKEND_PORT=8000
BACKEND_HOST=localhost

# Frontend server configuration
FRONTEND_PORT=3000
FRONTEND_HOST=localhost

# API URL (frontend uses this to connect to backend)
API_URL=http://localhost:8000

# Database
DATABASE_URL=postgres://postgres:dev@localhost:5432/myapp

# Other settings
DEBUG=true
SECRET_KEY=dev-secret-key-replace-in-production
